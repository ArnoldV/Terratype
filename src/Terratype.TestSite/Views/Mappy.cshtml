@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Mappy>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = null;
    var map = Model.Content.Map1;

    var GMapProvider = (Terratype.Providers.GoogleMapsV3) Model.Content.Map

}


<fieldset>
    <legend>
        Map 1
    </legend>

    <dl>
        <dt>
            Zoom
        </dt>
        <dd>
            @Model.Content.Map1.Zoom
        </dd>

        <dt>
